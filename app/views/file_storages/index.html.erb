<p id="notice"><%= notice %></p>

<h1>Listing File Storages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_storages.each do |file_storage| %>
      <tr>
        <td><%= link_to 'Show', file_storage %></td>
        <td><%= link_to 'Edit', edit_file_storage_path(file_storage) %></td>
        <td><%= link_to 'Destroy', file_storage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File storage', new_file_storage_path %>
