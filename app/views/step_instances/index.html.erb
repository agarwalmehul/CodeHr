<p id="notice"><%= notice %></p>

<h1>Listing Step Instances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @step_instances.each do |step_instance| %>
      <tr>
        <td><%= link_to 'Show', step_instance %></td>
        <td><%= link_to 'Edit', edit_step_instance_path(step_instance) %></td>
        <td><%= link_to 'Destroy', step_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Step instance', new_step_instance_path %>
